{
  "transform": {
    ".(js|jsx)$": "babel-jest",
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "../jest/mediaFileTransformer.js"
  },
  "rootDir": "../src",
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleDirectories": [
    "node_modules",
    "src",
    "tests"
  ],
  "setupTestFrameworkScriptFile": "../jest/setupTests.js",
  "moduleNameMapper": {
     "\\.(css|less|scss|sass)$": "identity-obj-proxy"
  },
  "testEnvironment": "node"
}
